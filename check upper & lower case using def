def count_upper_lower(s):
    upper_count = 0
    lower_count = 0
    
    for char in s:
        if char.isupper():
            upper_count += 1
        elif char.islower():
            lower_count += 1
    
    print(f"No. of Upper case characters : {upper_count}")
    print(f"No. of Lower case characters : {lower_count}")

# Sample String
sample_string = 'Anubhav Singh'
count_upper_lower(sample_string)
